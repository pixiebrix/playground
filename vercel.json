{
	"github": {
		"silent": true
	},
	"trailingSlash": true,
	"rewrites": [
		{
			"source": "/create-react-app/(.*)",
			"destination": "/create-react-app/index.html"
		},
		{
			"source": "/react-admin/(.*)",
			"destination": "/react-admin/index.html"
		},
		{
			"source": "/ngx-admin/(.*)",
			"destination": "/ngx-admin/index.html"
		}
	]
}
